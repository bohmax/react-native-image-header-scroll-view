{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react",
    "outDir": "./lib",
    "strict": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true
  },
  "exclude": [
    "node_modules",
    "example",
    "lib"
  ],
  "include": [
    "src"
  ],
  "extends": "expo/tsconfig.base"
}
